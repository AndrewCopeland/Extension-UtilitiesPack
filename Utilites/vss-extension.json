{
    "manifestVersion": 1,
    "id": "utilitytasks",
    "name": "Release Management Utility tasks",
    "version": "0.0.2",
    "publisher": "kishorebv",
    "description": "Utility tasks for Release Management",
    "public": false,
    "icons": {
        "default": "images/utilitytasks-logo.png",
        "large": "images/utilitytasks-logo-large.png"
    },
    "categories": [
        "Build and release"
    ],
    "tags": [
        "utility",
        "tasks"
    ],
	"links": {
			
	},
    "branding": {
        "color": "#f6f7fb",
        "theme": "light"
    },	
    "screenshots": [
        {
            "path": "images/screen1.png"
        },
        {
            "path": "images/screen2.png"
        },
        {
            "path": "images/screen3.png"
        }
    ],
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "files": [
      {
        "path": "Tokenizer"
      },
      {
        "path": "Powershell++"
      },
      {
        "path": "Shell++"
      },
      {
        "path": "zip"
      },
      {
        "path": "unzip"
      },
      {
        "path": "images",
        "addressable": true
      }
    ],
    "scopes": [
        "vso.build_execute",
        "vso.serviceendpoint_manage"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "task-tokenizer",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Tokenizer"
            }
        },
        {
            "id": "task-PSpp",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Powershell++"
            }
        },
        {
            "id": "task-Shellpp",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Shell++"
            }
        },
        {
            "id": "task-zip",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "zip"
            }
        },
        {
            "id": "task-unzip",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "unzip"
            }
        }
    ]
}
